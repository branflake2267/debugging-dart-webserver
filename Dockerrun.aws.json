  {
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "web-app",
      "host": {
        "sourcePath": "/var/app/current/"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "web-app",
      "image": "google/dart",
      "essential": true,
      "memory": 256,

      "mountPoints": [
        {
          "sourceVolume": "web-app",
          "containerPath": "/app",
          "readOnly": true
        }
      ],

      "command": [
        "dart",
        "/app/server/lib/server.dart"
      ],

      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ]
    }
  ]
}
